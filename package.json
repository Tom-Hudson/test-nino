{
  "name": "test-nino",
  "version": "1.1.2",
  "sideEffects": false,
  "description": "The fastest UK National Insurance number generator (nino). Generates a random valid UK NI number in accordance with NIM39110 guidelines on Gov.uk",
  "homepage": "https://github.com/Tom-Hudson/test-nino#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Tom-Hudson/test-nino"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean --minify",
    "watch": "npm run build -- --watch src",
    "lint": "ts-standard ./src",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest"
  },
  "keywords": [
    "nino",
    "national",
    "insurance",
    "national insurance",
    "nino generator",
    "ni number generator",
    "national insurance generator",
    "ni",
    "uk",
    "generator",
    "test",
    "testing",
    "fake",
    "random",
    "random_uk_nino",
    "fake-nino",
    "fng-nino-tools"
  ],
  "author": {
    "name": "Tom Hudson",
    "email": "email@tom-hudson.uk",
    "url": "https://tom-hudson.uk"
  },
  "license": "GNU GPLv3",
  "devDependencies": {
    "@types/jest": "^29.0.3",
    "jest": "^29.1.1",
    "ts-jest": "^29.0.2",
    "ts-standard": "^11.0.0",
    "tsup": "^6.2.3",
    "typescript": "^4.8.4"
  }
}
